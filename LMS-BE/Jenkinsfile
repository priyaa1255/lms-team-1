pipeline {
    agent any 
    stages {
        stage('Build') {
            steps {
                sh 'sudo docker rmi -f backend'
                sh 'cd LMS-BE && sudo docker build -t backend .'
            }
        }
        stage('run') {
            steps {
                sh 'sudo docker run -dt --name lmsbe -p 9090:8080 backend'
            }
        }
    }
}
