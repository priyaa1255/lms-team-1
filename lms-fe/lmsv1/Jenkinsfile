pipeline {
    agent any
    stages {
        stage ('Docker Build') {
            steps {
                sh 'cd lms-team-1-praneeth/lms-fe && docker build -t frontend .'
            }
        }
        stage ('container') {
            steps {
                sh 'docker run -dt --name fe -p 80:80 frontend'
            }
        }
    }
}
