pipeline {
    agent any
    stages {
        stage ('Docker Build') {
            steps {
                sh 'cd lms-fe/lmsv1 && sudo docker build -t frontend .'
            }
        }
        stage ('docker run') {
            steps {
                sh 'sudo docker container run -dt --name fe -p 80:80 frontend'
            }
        }
    }
}
